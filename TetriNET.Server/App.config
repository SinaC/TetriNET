<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5" />
    </startup>
  <system.serviceModel>
    <services>
      <service name = "TetriNET.Server.GameServer">
        <endpoint
           kind = "udpDiscoveryEndpoint"
            />
        <endpoint
           kind = "mexEndpoint"
           address = "MEX"
           binding = "mexTcpBinding"
            />
      </service>
    </services>
    <behaviors>
      <serviceBehaviors>
        <behavior>
          <serviceMetadata/>
          <serviceDiscovery/>
        </behavior>
      </serviceBehaviors>
    </behaviors>
  </system.serviceModel>
</configuration>